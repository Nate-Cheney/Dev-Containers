{
    "name": "LaTeX Dev Container",
    "build": {
        "context": "..",
        "dockerfile": "Dockerfile"
    },
    {
    "features": {
        "ghcr.io/devcontainers-extra/features/tmux-apt-get:1": {}
        "ghcr.io/devcontainers-extra/features/neovim-apt-get:1": {}
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "james-yu.latex-workshop",
                "tomoki1207.pdf"
            ]
        }
    }
}
